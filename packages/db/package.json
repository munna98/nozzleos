    "prisma": {
        "seed": "tsx prisma/seed.ts"
    },
    "dependencies": {
        "@prisma/client": "^6.1.0"
    },
    "devDependencies": {
        "@types/bcryptjs": "^3.0.0",
        "@types/node": "^25.0.9",
        "bcryptjs": "^3.0.3",
        "prisma": "^6.1.0",
        "tsx": "^4.19.2",
        "typescript": "^5.7.3"
    }
}